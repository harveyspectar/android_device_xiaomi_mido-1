<!-- Hardware 'face' keys present on the device, stored as a bit field.
         This integer should equal the sum of the corresponding value for each
         of the following keys present:
          1 - Home
          2 - Back
          4 - Menu
          8 - Assistant (search)
          16 - App switch
          32 - Camera
       For example, a device with Back, Home and AppSwitch keys would set this
       config to 19. -->
    <integer name="config_deviceHardwareKeys">19</integer>


    <!-- Hardware 'face' keys present on the device, stored as a bit field.
         This integer should equal the sum of the corresponding value for each
         of the following keys present:
          1 - Home
          2 - Back
          4 - Menu
          8 - Assistant (search)
          16 - App switch
          32 - Camera
       For example, a device with Back, Home and AppSwitch keys would set this
       config to 19. -->
    <integer name="config_deviceHardwareWakeKeys">19</integer>

    <!-- Button brightness -->
    <bool name="config_button_brightness_support">true</bool>
    <integer name="config_button_brightness_default">128</integer>

 <!-- Whether the device supports Smart Pixels --> 
<bool name="config_supportSmartPixels">true</bool>

<!--Systeminfo for quick settings header -->
   <string name="config_sysCPUTemp">/sys/class/thermal/thermal_zone0/temp</string>
   <string name="config_sysBatteryTemp">/sys/class/power_supply/battery/temp</string>
   <string name="config_sysGPUFreq">/sys/kernel/gpu/gpu_clock</string>
   <string name="config_sysGPULoad">/sys/kernel/gpu/gpu_busy</string>
   <integer name="config_sysCPUTempMultiplier">1000</integer>
   <integer name="config_sysBatteryTempMultiplier">10</integer>

</resources>
